{
  "name": "linc-profile-starter-kit",
  "version": "0.1.0",
  "description": "A starter kit to write your own profiles from scratch",
  "main": "dist/index.js",
  "author": "Erwin van der Koogh <hello@bitgenics.io>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint src test --fix",
    "clean": "rimraf dist",
    "build": "npm run clean; npm run lint; babel src -d dist -s",
    "dev": "npm run clean; babel src -d dist -s -w",
    "prepare": "npm run build"
  },
  "dependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "webpack": "^3.5.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "eslint": "^4.7.2",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "prettier": "^1.7.2",
    "rimraf": "^2.6.2"
  }
}
